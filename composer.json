{
    "name": "evansims/socialworth",
    "description": "A simple PHP library for determining the popularity of a URL across social networks.",
    "version": "2.0.2",
    "keywords": ["twitter", "facebook", "pinterest", "linkedin" ,"reddit", "google plus", "hackernews", "stumbleupon", "social network", "popularity", "analytics"],
    "homepage": "http://github.com/evansims/socialworth",
    "license": "MIT",
    "authors": [
        {
            "name": "Evan Sims",
            "homepage": "http://evansims.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Evansims\\Socialworth\\": "src/"
        }
    },
    "require": {
        "php": ">=5.4.0",
        "lib-curl": "*",
        "guzzlehttp/guzzle": "^5.3",
        "respect/validation": "^1.0",
        "symfony/validator": "~2.7"
    },
    "require-dev": {
        "satooshi/php-coveralls": "dev-master"
    }
}
